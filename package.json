{
  "name": "RNAppTS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --config ../../../../rn-cli.config.js",
    "test": "jest",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint -p ./tsconfig.json"
  },
  "dependencies": {
    "@types/react-redux": "^6.0.2",
    "react": "16.3.1",
    "react-native": "0.55.4"
  },
  "devDependencies": {
    "@types/jest": "23.0.0",
    "@types/react": "^16.3.17",
    "@types/react-native": "0.55.17",
    "@types/react-test-renderer": "16.0.1",
    "babel-jest": "23.0.1",
    "babel-preset-react-native": "4.0.0",
    "jest": "23.1.0",
    "prettier": "1.13.4",
    "react-native-typescript-transformer": "1.2.9",
    "react-test-renderer": "16.3.1",
    "ts-jest": "22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "2.9.1"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
