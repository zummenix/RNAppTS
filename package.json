{
  "name": "RNAppTS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --config ../../../../rn-cli.config.js",
    "test": "jest",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint -p ./tsconfig.json"
  },
  "dependencies": {
    "@types/react-redux": "^6.0.6",
    "@types/redux": "^3.6.0",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "typesafe-actions": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.47",
    "@babel/plugin-transform-flow-strip-types": "7.0.0-beta.47",
    "@types/jest": "23.0.0",
    "@types/react": "^16.3.17",
    "@types/react-native": "0.56.4",
    "@types/react-test-renderer": "16.0.1",
    "babel-jest": "23.4.2",
    "babel-preset-react-native-typescript": "^1.0.0-rc.1",
    "jest": "23.4.2",
    "prettier": "1.14.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "ts-jest": "23.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
